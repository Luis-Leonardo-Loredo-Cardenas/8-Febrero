syms x a

Sol=2*x+a-5

solve(Sol,x)

%-----------------------------

syms x a b

Sol=x.^2+a*x+b

y=solve(Sol,x)

%-----------------------------

syms x

Sol=2*exp(x)+3*cos(x)

y=solve(Sol,x)

%-----------------------------

syms x y c

A=[2 -3*c;
    c 2];

B=[5; 
    7]

IA=inv(A)

Sol=IA*B

%-----------------------------

syms x y

A=[3 -2;
    0 1+y];
B=[7-y;
    5];

IA=inv(A);

% solucion
Sol=IA*B;
